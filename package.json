    "husky": "^7.0.4",
    "jest": "^27.4.7",
    "lint-staged": "^12.1.7",
    "prettier": "2.5.1"
  },
  "lint-staged": {
    "**/*.js": [
      "eslint --fix",
      "prettier --write --ignore-unknown --loglevel=warn"
    ],
    "**/*.json": [
      "prettier --write --ignore-unknown --loglevel=warn"
    ]
